{
  "Authorization": {
    "Secret": "{{ secret }}"
  },
  "ServiceBus": {
    "StorageConnectionString": "Server=db_mssql,1433; Database={{ authentication_db_name }}; User Id={{ mssql.korp_user }}; Password={{ mssql.korp_password }}; MultipleActiveResultSets=true; Encrypt=False"
  },
  "UrlLoader": "{{ frontend.endpoints.frontend_url }}/*",
  "UrlRedirect": "{{ frontend.endpoints.frontend_url }}",
  "ConnectionStrings": {
    "DefaultConnection": "Server=db_mssql,1433; Database={{ authentication_db_name }}; User Id={{ mssql.korp_user }}; Password={{ mssql.korp_password }}; Encrypt=False"
  },
  "Provisioning": {
    "EnableSchemaProvisioning": "false"
  },
  "EnableEnvironmentDependentConnectionString": "false",
  "CertificatePath": "{{ cert_pfx_path }}",
  "CertificatePass": "{{ certs.pfx_passphrase }}",
  "AuthenticationProxy": {
    "ClientId": "{{ services_secrets['Viasoft.Legacy.Authentication'].ClientId }}",
    "ClientSecret": "{{ services_secrets['Viasoft.Legacy.Authentication'].ClientSecret }}",
    "Scope": "{{ services_secrets['Viasoft.Legacy.Authentication'].ClientId }}"
  },
  "ExternalAuthentication": {
    "Google": {
      "ClientId": "ClientId",
      "ClientSecret": "ClientSecret",
      "AllowedScopes": "https://www.googleapis.com/auth/calendar https://www.googleapis.com/auth/calendar.readonly https://www.googleapis.com/auth/calendar.events https://www.googleapis.com/auth/calendar.events.readonly https://www.googleapis.com/auth/calendar.settings.readonly"
    }
  }
}
