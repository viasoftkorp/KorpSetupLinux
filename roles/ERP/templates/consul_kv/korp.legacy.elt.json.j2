{
  "Authorization": {
    "Secret": "{{ secret }}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "sqlserver://{{ mssql.korp_user }}:{{ mssql.korp_password }}@db_mssql?database=%s&app name=korp.legacy.elt"
  },
  "PostgresqlConnectionString": "host={{ postgres_container_name }} user={{ postgres.korp_user }} password={{ postgres.korp_password }} dbname={{ service_vars.db.name }} port=5432",
  "ShouldMirrorMinioObjects": true,
  "ShouldMirrorMinioObjectsV1": true,
  "CourtYear": 2024,
  "OffSetSecondsFetchObjects": 60,
  "NotifyWebhooks": [
    {
      "ServiceName": "Viasoft.ELT",
      "Endpoint": "/Analystics/ELT/%s"
    }
  ],
}